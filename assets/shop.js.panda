if ((typeof shop) == 'undefined') {
	var shop = {};
}
shop.queryParams = {};
shop.cart = window.cart;

if (location.search.length) {
  for (var aKeyValue, i = 0, aCouples = location.search.substr(1).split('&'); i < aCouples.length; i++) {
    aKeyValue = aCouples[i].split('=');
    if (aKeyValue.length > 1) {
      shop.queryParams[decodeURIComponent(aKeyValue[0])] = decodeURIComponent(aKeyValue[1]);
    }
  }
}

shop.money = function(amount, money_format) {
	var pattern = /\{\{\s*(\w+)\s*\}\}/;

	function addSpaces(money) {
	    money = money.replace(/(\d+)(\d{3}[\. ]?)/g,'$1 $2');
	    money = money.replace(/(\d+)(\d{3}[\. ]?)/g,'$1 $2');
	    money = money.replace(/(\d+)(\d{3}[\. ]?)/g,'$1 $2');
	    return money;
	}

	switch(money_format.match(pattern)[1]) {
		case 'amount':
			value = addSpaces(parseFloat(amount).toFixed(2).toString());
			break;
		case 'amount_no_decimals':
			value = addSpaces(parseFloat(amount).toFixed(0).toString());
			break;
	}
	return money_format.replace(pattern, value); 
};

shop.resizeImage = function(image, width, height, crop) { 
	if (typeof(crop) != 'boolean') {
		crop = false; 
	}
	if (typeof(image) == 'string') {
		var src = image;
	}
	else if (typeof(image.src) == 'string') {
		var src = image.src;
	}
	
	try {
    if(width == 'original') { return src; }
    else {      
    	if (width == 'pico') {
    		var sizeStr = '_16_16';
    	}
    	else if (width == 'icon') {
    		var sizeStr = '_32_32';
    	}
    	else if (width == 'thumb') {
    		var sizeStr = '_50_50';
    	}
    	else if (width == 'small') {
    		var sizeStr = '_100_100';
    	}
    	else if (width == 'compact') {
    		var sizeStr = '_160_160';
    	}
    	else if (width == 'medium') {
    		var sizeStr = '_240_240';
    	}
    	else if (width == 'large') {
    		var sizeStr = '_480_480';
    	}
    	else if (width == 'grande') {
    		var sizeStr = '_600_600';
    	}
    	else {
    		var sizeStr = '_'+width+'_'+height;
    	}

    	if (crop) {
    		sizeStr += '_cropped'; 
    	}
      var matches = src.match(/(.*\/[\w\-\_\.]+)\.(\w{2,4})/);
      return matches[1] + sizeStr + '.' + matches[2];
    }    
  } catch (e) { return src; }
};

/*
    Modal Functions (used in checkout, could also be used for an AJAX cart)
*/
function showModal(id) {
    console.log("show modal", id);
    $('#modal-overlay').attr('data-current-modal', id);
    $('#'+id).show();
    sizeModal(false);
    //scrollTop();
    $(window).on('orientationchange', resizeWindow).on('resize', resizeWindow);
}
function hideModal() {
    $('.modal.is-visible').removeClass('is-visible').hide();
    $('.is-visible').removeClass('is-visible');
    $('body').removeClass('modal--visible');
    $('#modal-overlay').attr('data-current-modal', null);
    $(window).off('orientationchange', resizeWindow).off('resize', resizeWindow);
}
function resizeWindow() {
    sizeModal(true);
}
function sizeModal(isResizing) {
    var currentModalId = $('#modal-overlay').attr('data-current-modal');
    if (!isResizing) {
        $('#'+currentModalId).css('opacity', 0);
    }
    // Position modal by negative margin
    $('#'+currentModalId).css({
        'margin-left': - ($('#'+currentModalId).outerWidth() / 2),
        'opacity': 1
    });
    $('#'+currentModalId).addClass('is-visible');
    $('body').addClass('modal--visible');
}
function scrollTop() {
    console.log("scrollTop");
    if ($('body').scrollTop() > 0 || $('html').scrollTop() > 0) {
        $('html, body').animate({
            scrollTop: 0
        }, 250, 'swing');
    }
}

var origNavPosN = 65;
function createQuantitySelectors() {
    //Quantity Selector
    $('.quantity-selector:not(.input-group-field)').addClass('input-group-field').css({'border-radius':0}).wrap('<div class="input-group quantity-selector-wrapper"></div>').parent().prepend('<span class="input-group-btn"><a class="btn" data-type="minus">-</a></span>').append('<span class="input-group-btn"><a class="btn" data-type="plus">+</a></span>');
    
    $('.quantity-selector-wrapper .btn').off().on('click', function() {
        var input = $(this).parent().parent().find('input');
        var min = parseInt(input.attr('min'));
        var max = parseInt(input.attr('max'));
        
        if (isNaN(min)) {
            min = 1;
        }
        if (isNaN(max)) {
            max = null;
        }
        var newVal = parseInt(input.val());
        if ($(this).attr('data-type') == 'minus') {
            newVal--;
            if (newVal < min) {
                newVal = min;
            }
        }
        else {
            newVal++;
            if ((newVal > max) && (max !== null)) {
                newVal = max;
            }
        }
        input.val(newVal).change();
    });
}
$(function() {
    if ($('body').hasClass('template-index')) {
        origNavPosN = $(window).height()-55;
    }
    /*
        Make order summary sticky on custom checkout and thank you page.
    */
    if (($('html.lt-ie9').length < 1) && ($(window).width() >= 769) && ($('.sticky').length > 0)) {
        //Don't make sticky if <IE9 or device is small
        $(window).on('load', function() {
            $('.sticky').stick_in_parent({
                'parent':$('#checkout-grid'),
                'offset_top':24 
            });
        })
    }

    $('.social-sharing a').on('click', function(e) {
        e.preventDefault();
        var el = $(this),
            popup = el.attr('class').replace('-','_'),
            link = el.attr('href'),
            w = 700,
            h = 400;
    
        // Set popup sizes
        switch (popup) {
          case 'share-twitter':
            h = 300;
            break;
          case 'share-fancy':
            w = 480;
            h = 720;
            break;
          case 'share-google':
            w = 500;
            break;
        }
    
        window.open(link, popup, 'width=' + w + ', height=' + h);
    });
    
    
    //Make Youtube and Vimeo embeds responsive
    $('iframe[src*="youtube.com/embed"]').wrap('<div class="video-wrapper"></div>');
    $('iframe[src*="player.vimeo"]').wrap('<div class="video-wrapper"></div>');
    
    createQuantitySelectors();
    
    //Modal overlay
    $('<div/>', {
        'id':'modal-overlay'
    }).appendTo($('body')).on('click', hideModal);
    $('.modal .modal--content').prepend('<a class="modal--close"><i class="panda-icon-ui-close"></i></a>');
    $('.modal--close').on('click', hideModal);
    
    if ($('.flexslider').length > 0) {
        $(window).on('scroll', function() {
            $('.flexslider').css({"transform": "translate3d(0px, " + $(window).scrollTop()/3 + "px, 0px)"});
        });
    }
    
    
    $(window).on('scroll', function() {
        if (origNavPosN >= 0) {
            if ($(window).scrollTop() > origNavPosN) {
                $('.site-header').css({'position':'fixed','top':0});
                $('.site-header').addClass('scrolled-down');
            }
            else {
                $('.nav-bar').css({'position':'absolute','top':false});
                $('.site-header').removeClass('scrolled-down');

            }
        }
        else {
            origNavPosN = $(window).height()-55;
        }
    });
    
    
    if ($('.nav-bar').length > 0) {
    
        $('.nav-bar .site-nav .site-nav--has-dropdown').on('click', function(event) {
            event.preventDefault();
            $('#'+$(this).attr('points-to')).addClass('open');
            $('.nav-bar .site-nav').addClass('open');
        }); 
        $('.nav-bar [points-back="true"]').on('click', function(event) {
            event.preventDefault();
            $('.nav-bar [points-back="true"]').parent().parent().parent().removeClass('open');
            $('.nav-bar .site-nav').removeClass('open');
        });
        
        $('.nav-bar ul').on('scroll', function() {
            if ($(this).scrollLeft() > 50) {
                $(this).parent().addClass('scrolled');
            }
            else {
                $(this).parent().removeClass('scrolled');
            }
        });
        
        $(window).on('resize', checkNavbarWidths);
        checkNavbarWidths();
    }
    $('#menuToggle').on('click', function(e) {
        e.preventDefault();
        console.log("menuToggle");
        $('body').addClass('modal--visible');
        $('#navigation').addClass('is-visible');
    });
    $('#searchToggle').on('click', function(event) {
        event.preventDefault();
        $('body').addClass('modal--visible');
        $('#search').addClass('is-visible');
        $('#search .input-group-field').focus();
    });
    
    
    {% if settings.popup_cart_enable %}
    $('#cartToggle').on('click', function(event) {
        event.preventDefault();
        $('body').addClass('modal--visible');
        $('#cart').addClass('is-visible');
    });
    $('form[name="cart_add"]').on('submit', function(event) {
        event.preventDefault();
        $.post('/cart/add.json', $( this ).serialize(), function(response) {
        	$.get('/cart.json', {}, function(response) {
        	    shop.cart = response;
        	    buildCart();
        	    $('body').addClass('modal--visible');
                $('#cart').addClass('is-visible');
            }, 'json');
        }, 'json');
    });
    buildCart();
    {% endif %}

    resizeImages();
    
    initLoadMore();
});
function resizeImages() {
    $('img[data-original-url]').not('[src]').each(function() {
        var width = $(this).closest('.grid-item').width();
        if (width < 1) {
            $(this).attr('src', $(this).attr('data-original-url'));
        }
        else {
            width = Math.ceil(width/100)*100;
            $(this).attr('src', shop.resizeImage($(this).attr('data-original-url'), width, 0));
        }
    });
}
function initLoadMore() {
    $('#loadmore').off().on('click', function(event) {
        event.preventDefault();
        $.get($(this).attr('href'), {}, function (html) {
            $('.entries').append($(html).find('.entries').html());
            $('#loadmore').parent().html($(html).find('#loadmore').parent().html());
            initLoadMore();
            resizeImages();
        }, 'html');
    });
}
function buildCart() {
    if (shop.cart && shop.cart.line_items.length > 0) {
        $('.cart-subtotal--price').text(shop.money(shop.cart.subtotal_price, {{ shop.money_format | json }}));
        $('.cart-summary').show();
        $('.cart-menu').empty();
        $('#cart').find('.checkout-btn').attr('disabled',false).text({{ lang.check_out | def:'Check out' | json }});
        $('#cart .cart-subtotal').attr('disabled', false);
        for (var i = 0; i < shop.cart.line_items.length; i++) {
            var line_item = shop.cart.line_items[i];
            var row = $('<div/>', {
                'class':'grid cart-line-item',
            }).appendTo($('.cart-menu'));
            
            $('<div/>', {
                'class':'grid-item small--one-quarter medium--one-quarter large--one-quarter text-center',
                'html':'<a href="'+line_item.url+'?variant='+line_item.variant_id+'"><img src="'+((line_item.featured_image !== null) ? shop.resizeImage(line_item.featured_image, 80, 120):'//cdn.pandacommerce.net/static/global/gfx/no-image-50.gif')+'" /></a>'
            }).appendTo(row);
            
            var prodInfo = $('<div/>', {
                'class':'grid-item small--three-quarters medium--three-quarters large--three-quarters'
            }).appendTo(row);
            
            $('<div/>', {
                'class':'h6',
                'html':'<a href="'+line_item.url+'?variant='+line_item.variant_id+'">'+line_item.product.title+'</a>'
            }).appendTo(prodInfo);
            if (line_item.product.variants.length > 1) {
                $('<div/>', {
                    'text':line_item.variant.title
                }).appendTo(prodInfo);
            }
            var quantitydiv = $('<div/>', {
                'class':'left'
            }).appendTo(prodInfo);
            $('<input/>', {
                'type':'text',
                'class':'quantity-selector',
                'name':'updates[]',
                'pattern':'[0-9]*',
                'data-line':i+1,
                'data-id':line_item.variant_id,
                'min':0,
                'autocomplete':'off',
                'value':line_item.quantity
            }).on('focus', function() {
                $(this).select();
            }).on('change', function() {
                $(this).attr('disabled','disabled');
                $(this).parent().parent().find('a').attr('disabled','disabled').off();
                $('#cart').find('.checkout-btn').attr('disabled','disabled').html('<div class="spinner center"></div>');
                $('#cart .cart-subtotal').attr('disabled', true);
                $.post('/cart/change.json', {'line':$(this).attr('data-line'),'quantity':$(this).val()}, function(response) {
                    $.get('/cart.json', {}, function(response) {
                	    shop.cart = response;
                	    buildCart();
                    }, 'json');
                }, 'json');
            }).appendTo(quantitydiv);
            $('<div/>', {
                'class':'right',
                'text':shop.money(line_item.price, {{ shop.money_format | json }})
            }).appendTo(prodInfo);
        }
        createQuantitySelectors();
        $('body').addClass('have_items');
    }
    else {
        //Empty Cart
        $('.cart-summary').hide();
        $('body').removeClass('have_items');
        $('.cart-menu').html('<p>'+{{ lang.it_appears_that_your_cart_is_empty | def: 'It appears that your cart is currently empty.' | json }}+'</p>');
    }
}

// add to cart
$(function(){
    var addToCartTimer, inProgress;
    
    $('a.add-to-cart, button.add-to-cart, input.add-to-cart').on('click',function(e){
        e.preventDefault();
        e.stopPropagation();
        
        var self = $(this),
            variant_id,
            quantity;
        
        if(document.forms.cart_add){
            variant_id = document.forms['cart_add'].id.value;
            quantity = document.forms.cart_add.quantity.value;
        } else {
            variant_id = self.attr('data-variant-id');
            quantity = self.attr('data-quantity'); 
        }
        
        if(!variant_id) return false;
        
        quantity = parseInt(quantity,10);
        
        if(!quantity || isNaN(quantity)) quantity = 1;
        
        if(!inProgress && self.attr('disabled') == null){
            setDisabled(true);
            // clears inProgress incase of error
            addToCartTimer = window.setTimeout(function(){
                setDisabled(false);
            },5000);
            $.post('/cart/add.json', {
                "id":variant_id,
                "quantity": quantity
            }, function(response) {
                $.ajax({
                    url:'/cart.json',
                    method: 'get',
                    success: function(response) {
                        window.clearTimeout(addToCartTimer);
                        addToCartTimer = null;
                        setDisabled(false);
                        shop.cart = response;
                        buildCart();
                        $('#cart .cart-menu').scrollTop($('#cart .cart-menu').height() + 48);
                        // show cart unless it's visible
                        if(!$('#cart').hasClass('is-visible')){
                            $('#cartToggle').trigger('click');
                            setTimeout(function(){
                                if(!$('#cart:hover').length){ 
                                    if($('#cart').hasClass('is-visible')){
                                        $('#cart .modal--close').trigger('click');
                                    }
                                }
                            },3000);
                        }
                    }
                }, 'json');
            }, 'json');
        }
        
        function setDisabled(status){
            if(status) {
                inProgress = true;
                self.attr('disabled','disabled')
                    .data('content',self.html())
                    .html('<div class="spinner center"></div>');
            } else {
                inProgress = false;
                self.removeAttr('disabled')
                    .html(self.data('content'));
            }
        }
    });
    
    $('#cart').mouseleave(function(){
        $('#cart .modal--close').trigger('click');
    });
});
    
function checkNavbarWidths() {
    $('.overlay-content-wrapper .header-logo').width($('body').width()); //Fix for mobile since div is inside display: table
    $('.nav-bar >div').each(function() {
        var totwidth = 0;
        $(this).find('li').each(function() {
            totwidth += $(this).outerWidth();  
        });
        if (totwidth > $(document).width()) {
            $(this).addClass('scrollable');
        }
        else {
            $(this).removeClass('scrollable');
        }
    });
}
